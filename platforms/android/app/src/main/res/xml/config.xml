<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.qcordovausertestapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <feature name="QUsersCordova">
        <param name="android-package" value="com.q.users.cordova.plugin.QUsersCordova" />
    </feature>
    <feature name="QUsersUICordova">
        <param name="android-package" value="com.q.users.cordova.plugin.QUsersUICordova" />
    </feature>
    <name>HelloCordova</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
